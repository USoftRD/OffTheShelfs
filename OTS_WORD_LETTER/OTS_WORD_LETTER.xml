;foreach&#x20;(XmlNode&#x20;field&#x20;in&#x20;r.ChildNodes)&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FillControlData(template_row,&#x20;field);&#xD;&#xA;&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;rows[0].ParentNode.AppendChild(template_row);&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;}&#xD;&#xA;&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;//&#x20;Remove&#x20;the&#x20;prototype&#x20;row&#x20;and&#x20;add&#x20;all&#x20;of&#x20;the&#x20;newly&#x20;constructed&#x20;rows.&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;rows[0].ParentNode.RemoveChild(rows[0]);&#xD;&#xA;&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;//&#x20;Remove&#x20;the&#x20;content&#x20;control&#x20;for&#x20;the&#x20;table&#x20;and&#x20;replace&#x20;it&#x20;with&#x20;its&#x20;contents.&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;XmlNodeList&#x20;contents&#x20;=&#x20;ctrl.SelectNodes(&quot;w:sdtContent&quot;,&#x20;nsmgr);&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;if&#x20;(contents.Count&#x20;==&#x20;0)&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;return;&#xD;&#xA;&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;XmlNodeList&#x20;content_items&#x20;=&#x20;contents[0].ChildNodes;&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;ArrayList&#x20;childrenList&#x20;=&#x20;new&#x20;ArrayList();&#xD;&#xA;&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;foreach&#x20;(XmlNode&#x20;child&#x20;in&#x20;content_items)&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;childrenList.Add(child);&#xD;&#xA;&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;foreach(XmlNode&#x20;child&#x20;in&#x20;childrenList)&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;ctrl.ParentNode.InsertBefore(child,&#x20;ctrl);&#xD;&#xA;&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;ctrl.ParentNode.RemoveChild(ctrl);&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;XmlNodeList&#x20;sdts&#x20;=&#x20;table.SelectNodes(&quot;descendant::w:sdt&quot;,&#x20;nsmgr);&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;foreach&#x20;(XmlNode&#x20;s&#x20;in&#x20;sdts)&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;s.ParentNode.RemoveChild(s);&#xD;&#xA;&#x9;&#x9;&#x9;}&#xD;&#xA;&#x9;&#x9;}&#xD;&#xA;&#x9;&#x9;else&#xD;&#xA;&#x9;&#x9;{&#x20;&#xD;&#xA;&#x9;&#x9;&#x9;//&#x20;Replace&#x20;the&#x20;run&#x20;in&#x20;the&#x20;structured&#x20;data&#x20;type&#