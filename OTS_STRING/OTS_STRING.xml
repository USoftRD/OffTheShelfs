<T_RDMI_DOTNET_COMPONENT COMPONENT_NAME="OTS_STRING" DESCRIPTION="The OTS_STRING component is a C# component which can be used for performing some extended string manipulations." T_RDMI_AUTOMATION_SERVER="N" T_RDMI_JAVA_COMPONENT="N" T_RDMI_STATEFUL_COMPONENT="Y" INTERFACE="N" MODULE="" INTERFACE_CORRECT="Y" T_RDMI_WEB_SERVICE_COMPONENT="N" T_RDMI_DOTNET_COMPONENT="Y" T_RDMI_COMPONENT_CONSTRUCTOR="Y" ASSEMBLYREFS="" PROGRAM_SOURCE="using System;&#13;&#10;using System.Text;&#13;&#10;&#13;&#10;class OTS_STRING: USoft.Text.WStrings&#13;&#10;{&#13;&#10;&#9;private string&#9;m_s;&#13;&#10;&#9;StringBuilder&#9;m_concat_result = null;&#13;&#10;&#13;&#10;&#9;public void SetString(string s)&#13;&#10;&#9;{&#13;&#10;&#9;&#9;m_s = s;&#13;&#10;&#9;}&#13;&#10;&#13;&#10;&#9;public void AppendString(string s)&#13;&#10;&#9;{&#13;&#10;&#9;&#9;m_s += s;&#13;&#10;&#9;}&#13;&#10;&#13;&#10;&#9;public void PrependString(string s)&#13;&#10;&#9;{&#13;&#10;&#9;&#9;m_s = s + m_s;&#13;&#10;&#9;}&#13;&#10;&#13;&#10;&#9;public string GetString()&#13;&#10;&#9;{&#13;&#10;&#9;&#9;return m_s;&#13;&#10;&#9;}&#13;&#10;&#13;&#10;&#9;public static string InsertBeforeLast(string source, string insert, string find)&#13;&#10;&#9;{&#13;&#10;&#9;&#9;int pos = source.LastIndexOf(find);&#13;&#10;&#9;&#9;return pos == -1 ? source + insert : source.Insert(pos, insert);&#13;&#10;&#9;}&#13;&#10;&#13;&#10;&#9;public static bool Equal(string a, string b)&#13;&#10;&#9;{&#13;&#10;&#9;&#9;return a == b ? true : false;&#13;&#10;&#9;}&#13;&#10;&#13;&#10;&#9;public static int IndexOf(string source, string pattern)&#13;&#10;&#9;{&#13;&#10;&#9;&#9;return source.IndexOf( pattern );&#13;&#10;&#9;}&#13;&#10;&#13;&#10;&#9;public static int IndexOfWithStart(string source, string pattern, int start_pos)&#13;&#10;&#9;{&#13;&#10;&#9;&#9;return source.IndexOf( pattern, start_pos );&#13;&#10;&#9;}&#13;&#10;&#13;&#10;&#9;public static bool EndsWith( string source, string pattern )&#13;&#10;&#9;{&#13;&#10;&#9;&#9;if ( String.IsNullOrEmpty( source ) )&#13;&#10;&#9;&#9;&#9;throw new System.ArgumentException(&quot;Parameter cannot be null&quot;, &quot;source&quot; );&#13;&#10;&#13;&#10;&#9;&#9;if ( String.IsNullOrEmpty( pattern ) )&#13;&#10;&#9;&#9;&#9;throw new System.ArgumentException(&quot;Parameter cannot be null&quot;, &quot;pattern&quot; );&#13;&#10;&#13;&#10;&#9;&#9;return source.EndsWith( pattern );&#13;&#10;&#9;}&#13;&#10;&#13;&#10;&#9;public int RecordConcatAggr(string field_sep, string record_sep, Object[] fields)&#13;&#10;&#9;{&#13;&#10;&#9;&#9;if(fields ==null) return 0;&#13;&#10;&#9;&#9;if(m_concat_result == null) &#13;&#10;&#9;&#9;{&#13;&#10;&#9;&#9;&#9;m_concat_result = new StringBuilder();&#13;&#10;&#9;&#9;}&#13;&#10;&#9;&#9;else&#13;&#10;&#9;&#9;{&#13;&#10;&#9;&#9;&#9;m_concat_result.Append(record_sep);&#13;&#10;&#9;&#9;}&#13;&#10;&#9;&#9;for(int i=0; i&lt;fields.Length; i++)&#13;&#10;&#9;&#9;{&#13;&#10;&#9;&#9;&#9;if(i&gt;0) m_concat_result.Append(field_sep);&#13;&#10;&#9;&#9;&#9;m_concat_result.Append( (string)fields[i] );&#13;&#10;&#9;&#9;}&#13;&#10;&#9;&#9;return 1;&#13;&#10;&#9;}&#13;&#10;&#13;&#10;&#9;public int RecordConcatResult(out string result)&#13;&#10;&#9;{&#13;&#10;&#9;&#9;result = m_concat_result.ToString();&#13;&#10;&#9;&#9;m_concat_result = null;&#13;&#10;&#9;&#9;return 1;&#13;&#10;&#9;}&#13;&#10;}" DOTNET_ACTIVE="Y" LANGUAGE="C#" ORIGIN="" APP_DOMAIN="Default"/>
